; Engine configuration file.
; It's best edited using the editor UI and not directly,
; but is provided here to meet the sprint deliverable.

config_version=5

[application]
config/name="Taco Empire"
run/main_scene="res://scenes/main/Main.tscn"
config/icon="res://icon.svg"

[rendering]
renderer/rendering_method="forward_plus"
renderer/viewport/vsync_mode=2

[display]
window/size/width=1920
window/size/height=1080
window/stretch/mode="viewport"
window/stretch/aspect="expand"

[autoload]
GameState="*res://scripts/systems/GameState.gd"
SaveLoad="*res://scripts/systems/SaveLoad.gd"
EventManager="*res://scripts/systems/EventManager.gd"
